generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Order {
  id            String   @id
  restaurantId String
  status        String
  total         Float
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
}

model ProcessedEvent {
  eventId     String   @id
  processedAt DateTime @default(now())
}
